# Composio API Configuration
# Get your API key from https://app.composio.dev/settings/api-keys
VITE_COMPOSIO_API_KEY=your_composio_api_key_here

# Composio Auth Config IDs
# These are created in the Composio dashboard under Auth Configs
# Each integration requires an OAuth2 auth config to be created first

# GitHub Integration
# Create a GitHub OAuth2 auth config in Composio dashboard
# Format: ac_xxxxxxxxxxxxxxxxxxxx
VITE_COMPOSIO_GITHUB_AUTH_CONFIG_ID=ac_your_github_auth_config_id

# Google Drive Integration
# Create a Google Drive OAuth2 auth config in Composio dashboard
# Format: ac_xxxxxxxxxxxxxxxxxxxx
VITE_COMPOSIO_GOOGLEDRIVE_AUTH_CONFIG_ID=ac_your_googledrive_auth_config_id

# Asana Integration
# Create an Asana OAuth2 auth config in Composio dashboard
# Format: ac_xxxxxxxxxxxxxxxxxxxx
VITE_COMPOSIO_ASANA_AUTH_CONFIG_ID=ac_your_asana_auth_config_id


VITE_SUPERVISOR_URL=""
VITE_EVALS_URL=""